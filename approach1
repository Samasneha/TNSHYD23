package approach1;

public class Sneha {
	int b=4;//instance variable
	static int c=6;//static variable
	int show() {
		return 0;
		
	}
	static void display() {//static method
      System.out.println(c);
	}
	public static void main(String[] args) {
		Sneha obj=new Sneha();
		 System.out.println(obj.b);
		 obj.show();
		 System.out.println(Sneha.c);
		 Sneha.display();
	}
}
